<% for( let i = 0; i < listAPI.length; i++ ) { %>
    <% if (listAPI[i].backdrop_path === null) { %>
        <p></p>
    <% } else { %>
        <p> <img src=<%= `https://image.tmdb.org/t/p/w500/${listAPI[i].backdrop_path}`%>> </p> 
    <% } %>
    
    <p><a href="/movies/lists/<%= listAPI[i].id %> "><%= listAPI[i].title %>  </a></p>
    <p><%= listAPI[i].overview %> </p
<% } %>

